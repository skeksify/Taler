<script type="text/javascript" src="/rj/jquery.jsPlumb-1.7.2-min.js"></script>
<script type="text/javascript" src="/rj/storyline_editor.js"></script>
<link href="/rs/storyline_editor.css" rel="stylesheet"/>
<div class="action-box" id="action_box_id">
    <div class="top-bar">
        <div class="title">
            Add New Entry
        </div>
    </div>
    <div class="wrappers-wrapper">
        <div class="entry-form-wrapper fw">
            <form role="form">
                <fieldset>
                    <!-- Text input-->
                    <div class="control-group">
                        <label class="control-label" for="new_entry_labelI">Label:</label>
                        <div class="controls">
                            <input id="new_entry_labelI" type="text" placeholder="Personal Use" class="form-control" required="">
                        </div>
                        <br/>
                        <label class="control-label" for="new_entry_speechI">Speech:</label>
                        <div class="controls">
                            <textarea id="new_entry_speechI" class="form-control speech" required=""></textarea>
                        </div>
                    </div>
                </fieldset>
            </form>
        </div>
        <div class="reply-form-wrapper fw">
            <form role="form">
                <fieldset>
                    <!-- Text input-->
                    <div class="control-group">
                        <label class="control-label" for="new_reply_labelI">Label:</label>
                        <div class="controls">
                            <input id="new_reply_labelI" type="text" placeholder="Personal Use" class="form-control" required="">
                        </div>
                        <br/>
                        <label class="control-label" for="new_reply_speechI">Speech:</label>
                        <div class="controls">
                            <textarea id="new_reply_speechI" class="form-control speech" required=""></textarea>
                        </div>
                    </div>
                </fieldset>
            </form>
        </div>
        <div class="board-form-wrapper fw">
            <form role="form">
                <fieldset>
                    <!-- Text input-->
                    <div class="control-group">
                        <label class="control-label" for="new_board_titleI">Title:</label>
                        <div class="controls">
                            <input id="new_board_titleI" type="text" placeholder="Personal Use" class="form-control" required="">
                        </div>
                        <br/>
                        <div class="">
                            <label class="radio-inline" for="bType-0">
                                <input type="radio" name="new_board_type" id="bType-0" value="con" checked="checked">
                                Conversation
                            </label>
                            <label class="radio-inline" for="bType-1">
                                <input type="radio" name="new_board_type" id="bType-1" value="area">
                                Area
                            </label>
                        </div>
                        <br/>
                        <label class="control-label" for="new_board_descI">Description:</label>
                        <div class="controls">
                            <textarea id="new_board_descI" class="form-control speech" required=""></textarea>
                        </div>
                    </div>
                </fieldset>
            </form>
        </div>
    </div>
    <div class="msg"></div>
    <div class="clr"></div>
    <div class="bottom-bar">
        <div class="not-last cancel">
            Cancel
        </div><div class="ok">
            Ok
        </div>
    </div>
</div>
<div class=" theme-showcase" role="main">
    <div id="playground">
        <!-- Form Name -->
        <h3><%=headerTitle%></h3>
        <div class="boards"></div>
        <input type="button" class="btn btn-add-board" value="New Board" style="margin-top: 20px">
    </div>
</div>

